{
    "name" : "tekoway/symfony-rollbar",
    "description" : "Symfony-Rollbar bundle",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "victor",
        "email" : "cataragav.xid@gmail.com"
    }],
    "keywords" : [
        "tekoway symfony-rollbar bundle"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.6",
        "rollbar/rollbar": "^1.3",
        "symfony/event-dispatcher": ">=2.6",
        "symfony/http-kernel": ">=2.6",
        "symfony/security-core": ">=2.6",
        "symfony/debug": ">=2.6"
        
    },
    "require-dev": {
        "phpspec/phpspec": "~4",
        "phpunit/phpunit": "~5.7",
        "symfony/framework-bundle": "~2.6|~3.0",
        "symfony/security-bundle": "~2.6|~3.0",
        "doctrine/doctrine-bundle": "~1.4"
    },
    "autoload" : {
        "psr-4" : {
            "Tekoway\\Rollbar\\" : ""
        }
    },
    "extra" : {
    "branch-alias" : {
            "dev-master" : "master"
        }
    }
